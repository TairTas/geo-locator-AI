
{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": ".vercel/output",
    "resolveJsonModule": true
  },
  "include": ["api/**/*.ts", "types.ts"],
  "exclude": ["node_modules"]
}
